<div class="tabel-list"  >

  <% @device_maintain_log.each do |log|  %>
    <% device = log.keys[0]; status = log.values[0]%>
    <table >
      <tr>
        <th>详情</th>
        <th>设备编号</th>
        <th>设备品牌</th>
        <th>设备序列号</th>
        <th>设备类型</th>
        <th>设备型号</th>
        <th>使用部门</th>
        <th>设备状态</th>
        <th>购置时间</th>
        <th>维保到期时间</th>
      </tr>
      <tr>
        <td>
          <%= link_to_function '[+]', "showDetails('device_#{device.id}','status_#{device.id}')", :id=>"device_#{device.id}"%>
        </td>
        <td><%=h device.no %></td>
        <td><%=h device.brand %></td>
        <td><%=h device.sn %></td>
        <td><%=h device.device_type %></td>
        <td><%=h device.model %></td>
        <td><%=h device.department %></td>
        <td><%=h device.status %></td>
        <td><%=h device.start_using_time %></td>
        <td><%=h device.end_server_time %></td>
      </tr>
    </table>
    <div id="status_<%= device.no %>" style="display:none">
      <% @device_status_log=status %>
      <%= render :partial => "status_log", :object=>@device_status_log %>
    </div>

  <% end  %>

</div>

<br />
<div class="pagination">
  <%= will_paginate @device_maintain_log %>
</div>

