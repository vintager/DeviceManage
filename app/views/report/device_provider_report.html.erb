<div class = "tabel-list">
  <table>
    <tr>
      <th>厂商</th>
      <% for t in device_types %>
        <th><%=h t.name %></th>
      <% end %>
      <th>合计</th>
    </tr>
    <% for l in @list %>
      <tr>
        <td style="text-align: left"><%=h l[0] %></td>
        <% for count in l[1] %>
          <td><%=h count %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
</div>
<%= link_to '返回', :action=>"index" %>